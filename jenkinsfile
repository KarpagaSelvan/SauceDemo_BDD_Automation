pipeline{
	agent any
	stages{
		stage('build'){
			steps{
				echo "Build Process Successfully Completed!"
				bat 'mvn clean install'
				}
			}
		stage('deploy'){
			steps{
				echo "Deploy Process Successfully Completed!"
				bat "mvn test"
				}
			}	
		stage('test'){
			steps{
				echo "test Process Successfully Completed!"
				bat "mvn compile"
				}
			}		
		stage('release'){
			steps{
				echo "Release Process Successfully Completed!"
				}
			}
		}
	}				
			
			